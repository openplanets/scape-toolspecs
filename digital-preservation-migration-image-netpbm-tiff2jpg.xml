<?xml version="1.0" encoding="utf-8"?>
<tool xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://scape-project.eu/tool" xmlns:xlink="http://www.w3.org/1999/xlink" xsi:schemaLocation="http://scape-project.eu/tool schemas/tool-1.1_draft.xsd" schemaVersion="1.1" name="Netpbm" version="1.0.2" homepage="http://netpbm.sourceforge.net">
	<license name="Apache-2.0" uri="http://opensource.org/licenses/Apache-2.0" type="FLOSS"/>
	<installation>
		<operatingSystem operatingSystemName="Debian">
			<packageManager type="Dpkg">
				<config>netpbm</config>
			</packageManager>
			<dependency name="netpbm">
				<license name="GPL-2.0" uri="http://opensource.org/licenses/GPL-2.0" type="FLOSS"/>
			</dependency>
		</operatingSystem>
	</installation>
	<operations>
		<operation name="digital-preservation-migration-image-netpbm-tiff2jpg">
			<description>Converts TIFF to JPEG</description>
			<command>/usr/share/digital-preservation-migration-image-netpbm-tiff2jpg/digital-preservation-migration-image-netpbm-tiff2jpg.sh ${input} ${output}</command>
			<inputs>
				<input name="input" required="true">
					<description>Reference to input file</description>
				</input>
				<parameter name="params" required="false">
					<description>Additional conversion parameters</description>
				</parameter>
			</inputs>
			<outputs>
				<output name="output" required="true">
					<description>Reference to output file</description>
					<extension>jpg</extension>
				</output>
			</outputs>
		</operation>
	</operations>
</tool>
